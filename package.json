{
  "name": "meta_life",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-npm": "npm clean-install",
    "nAndp": "npm install && npx pod-install",
    "lib": "tsc",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "propagate-replacements": "propagate-replacement-fields --field=react-native",
    "build-backend-android": "./tools/build-backend.js --android --no-rust",
    "build-android-assets": "npm run lib && npm run propagate-replacements && npm run build-backend-android",
    "android-dev": "npm run build-android-assets && react-native run-android --variant=indieDebug",
    "run-android": "react-native run-android",
    "install-android": "adb install -r android/app/release/app-release.apk",
    "clean-ios": "cd ios && xcodebuild -workspace meta_life.xcworkspace -scheme meta_life clean && cd ..",
    "build-backend-ios": "./tools/build-backend.js --ios --no-rust",
    "run-ios": "react-native run-ios",
    "run-ios iPhone": "react-native run-ios --verbose --device \"iPhone\"",
    "run-ios Lonmee’s iPhone": "react-native run-ios --verbose --device \"Lonmee’s iPhone\"",
    "archive-ios-release": "npm run build-ios-assets && cd ios && xcodebuild -workspace meta_life.xcworkspace -scheme meta_life archive && cd ..",
    "deploy iPhone": "ios-deploy --id 70bf1f5287fd7166e2ca66ac061586535fafd4b9 --bundle /Users/lonmee/Library/Developer/Xcode/DerivedData/meta_life-bnzedzxygwdaftgseyvytmarzlhx/Build/Products/Debug-iphoneos/meta_life.app",
    "deploy Lonmee’s iPhone": "ios-deploy --id 00008030-000635D902F9802E --bundle /Users/lonmee/Library/Developer/Xcode/DerivedData/meta_life-bnzedzxygwdaftgseyvytmarzlhx/Build/Products/Debug-iphoneos/meta_life.app",
    "deploy Huooo’s iPhone": "ios-deploy --id 25b32ab84e5c01d74d2818a376f00570f55b0231 --bundle /Users/lonmee/Library/Developer/Xcode/DerivedData/meta_life-bnzedzxygwdaftgseyvytmarzlhx/Build/Products/Debug-iphoneos/meta_life.app",
    "build-ios-assets": "npm run lib && npm run propagate-replacements && npm run build-backend-ios",
    "ios-dev": "npm run build-ios-assets && react-native run-ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.11",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "buffer": "^6.0.3",
    "nodejs-mobile-react-native": "^0.6.4",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-device-info": "^8.4.5",
    "react-native-i18n": "^2.0.15",
    "react-native-os-staltz": "^2.2.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-ssb-client": "^7.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "propagate-replacement-fields": "^1.2.0",
    "react-test-renderer": "17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "ssb-typescript": "^2.6.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
