{
  "name": "meta_life",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-npm": "npm clean-install",
    "nAndp": "npm install && npx pod-install",
    "lib": "tsc",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "propagate-replacements": "propagate-replacement-fields --field=react-native",
    "build-backend-android": "./tools/build-backend.js --android --no-rust",
    "build-android-assets": "npm run lib && npm run propagate-replacements && npm run build-backend-android",
    "android-dev": "npm run build-android-assets && react-native run-android --variant=indieDebug",
    "run-android": "react-native run-android",
    "install-android": "adb install -r android/app/release/app-release.apk",
    "clean-ios": "cd ios && xcodebuild -workspace meta_life.xcworkspace -scheme meta_life clean && cd ..",
    "build-backend-ios": "./tools/build-backend.mjs --ios",
    "run-ios": "react-native run-ios",
    "run-ios iPhone": "react-native run-ios --verbose --device \"Lonmee’s iPhone SE\"",
    "run-ios Lonmee’s iPhone": "react-native run-ios --verbose --device \"Lonmee’s iPhone\"",
    "archive-ios-release": "npm run build-ios-assets && cd ios && xcodebuild -workspace meta_life.xcworkspace -scheme meta_life archive && cd ..",
    "deploy Lonmee’s iPhone": "ios-deploy --id 00008101-0016491834E9003A --bundle /Users/lonmee/Library/Developer/Xcode/DerivedData/meta_life-fkwejdqeokznizfnnldtlshmuxkf/Build/Products/Debug-iphoneos/meta_life.app",
    "deploy Huooo’s iPhone": "ios-deploy --id 25b32ab84e5c01d74d2818a376f00570f55b0231 --bundle /Users/lonmee/Library/Developer/Xcode/DerivedData/meta_life-fkwejdqeokznizfnnldtlshmuxkf/Build/Products/Debug-iphoneos/meta_life.app",
    "build-ios-assets": "npm run lib && npm run propagate-replacements && npm run build-backend-ios",
    "ios-dev": "npm run build-ios-assets && react-native run-ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.11",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "buffer": "^6.0.3",
    "ios-deploy": "^1.11.4",
    "nodejs-mobile-react-native": "^0.6.4",
    "pull-stream": "^3.6.14",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-device-info": "^8.4.8",
    "react-native-i18n": "^2.0.15",
    "react-native-os-staltz": "^2.2.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-ssb-client": "^7.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "ssb-serve-blobs": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/runtime": "^7.17.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.5.0",
    "eslint": "8.8.0",
    "jest": "^27.5.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "propagate-replacement-fields": "^1.3.0",
    "react-test-renderer": "17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "ssb-typescript": "^2.6.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  },
  "jest": {
    "preset": "react-native"
  }
